public class Encoding {
	private double input;
	private String str;

	public Encoding(double input) {
		this.input=input;
	}
	public Encoding(String str) {
		this.str=str;
	}
	
	public double strToDouble() {
		double result=0;
		for(int i=0;i<str.length();i++) {
			char cr=str.charAt(i);
			int end= cr;
			result+=end;
		}
		return result;
	}
}
